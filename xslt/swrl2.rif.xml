<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="RIFBLD_XML2PRES2.xsl"?>
<Document xmlns:xs="http://www.w3.org/2001/XMLSchema#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.w3.org/2007/rif#">
<payload>
<Group>

<sentence>
<Forall>
<declare>
<Var>x2</Var>
</declare>
<declare>
<Var>x1</Var>
</declare>
<declare>
<Var>x3</Var>
</declare>
<formula>
<Implies>
<if>
<And>
<formula>
<Atom>
<op>
<Const type="http://www.w3.org/2007/rif#iri">hasParent</Const>
</op>
<args ordered="yes">
<Var>x1</Var>
<Var>x2</Var>
</args>
</Atom>
</formula>
<formula>
<Atom>
<op>
<Const type="http://www.w3.org/2007/rif#iri">hasBrother</Const>
</op>
<args ordered="yes">
<Var>x2</Var>
<Var>x3</Var>
</args>
</Atom>
</formula>
</And>
</if>
<then>
<Atom>
<op>
<Const type="http://www.w3.org/2007/rif#iri">hasUncle</Const>
</op>
<args ordered="yes">
<Var>x1</Var>
<Var>x3</Var>
</args>
</Atom>
</then>
</Implies>
</formula>
</Forall>
</sentence>
</Group>
</payload>
</Document>

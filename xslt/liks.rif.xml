<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="RIFBLD_XML2PRES.xsl"?>
<Document xmlns:xs="http://www.w3.org/2001/XMLSchema#" xmlns:ruleml="http://ruleml.org/spec" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.w3.org/2007/rif#">
<payload>
	<Group>
		<Atom xmlns="">
		<op>
		<Const type="http://www.w3.org/2007/rif#iri">likes</Const>
		</op>
		<args ordered="yes">
		<Const type="http://www.w3.org/2007/rif#iri">Mary</Const>
		<Const type="http://www.w3.org/2007/rif#iri">wine</Const>
		</args>
		</Atom>
		
		<Implies xmlns="">
			<if>
				<Atom>
				<op>
				<Const type="http://www.w3.org/2007/rif#iri">likes</Const>
				</op>
				<args ordered="yes">
				<Var>x</Var>
				<Const type="http://www.w3.org/2007/rif#iri">wine</Const>
				</args>
				</Atom>
			</if>
			<then>
				<Atom>
				<op>
				<Const type="http://www.w3.org/2007/rif#iri">likes</Const>
				</op>
				<args ordered="yes">
				<Const type="http://www.w3.org/2007/rif#iri">John</Const>
				<Var>x</Var>
				</args>
				</Atom>
			</then>
		</Implies>
	</Group>
</payload>
</Document>